{
  "manifest_version":2,
  "name":"PKU IPGW Extra",
  "version":"1.0.0",
  "description":"Auto PKU IPGW connect helper",
  "icons":{
	"16":"images/PKU-IPGW-Extra-16.png",
	"48":"images/PKU-IPGW-Extra-48.png",
	"128":"images/PKU-IPGW-Extra-128.png"
  },
  "browser_action": {
    "default_icon": "images/PKU-IPGW-Extra-19.png",
	"default_title": "PKU IPGW Extra",
	"default_popup":"popup.html"
  },
  "permissions": [ 
	"https://its.pku.edu.cn:5428/",
	"notifications" 
  ],
  "web_accessible_resources": [
	"images/PKU-IPGW-Extra-32.png",
	"images/PKU-IPGW-Extra-128.png",
	"fonts/caecilialtstd-bold-webfont.woff",
	"fonts/caecilialtstd-italic-webfont.woff",
	"fonts/caecilialtstd-roman-webfont.woff"
  ]
}

